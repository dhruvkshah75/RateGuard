generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  apiKeys   ApiKey[]  
}

model ApiKey {
  id        String   @id @default(uuid())
  key       String   @unique
  userId    String
  limit     Int      @default(10)
  window    Int      @default(60)
  user      User     @relation(fields: [userId], references: [id])
}